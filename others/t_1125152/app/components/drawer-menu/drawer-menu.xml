          </StackLayout>
          <StackLayout class="menuItemIcon">
            <Label text="&#xf105;" class="icon" />
          </StackLayout>
        </StackLayout>
      </RL:Ripple>
    </StackLayout>
    <StackLayout row="8" className="menuItem main-color" orientation="horizontal"  tap="tapAboutUHNow"  style="margin-bottom:.5%;">
      <RL:Ripple rippleColor="#ff8888" class="drawerRipple"   tap="tapAboutUHNow">
