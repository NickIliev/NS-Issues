<android >

  <GridLayout id="drawer-content" rows="*,*,*,*,*,*,*,*,*" style="margin-bottom:12%;" xmlns:RL="nativescript-ripple">
    <StackLayout row="0" className="menuItem main-color" orientation="horizontal" style="margin-bottom:.5%;" tap="tapHome">
      <RL:Ripple rippleColor="#ff8888" class="drawerRipple"   tap="tapHome">
        <StackLayout orientation="horizontal">
          <StackLayout class="menuItemLabel">
            <Label text="HOME" />
          </StackLayout>
          <StackLayout class="menuItemIcon">
            <Label text="&#xf105;" class="icon" />
          </StackLayout>
        </StackLayout>
      </RL:Ripple>
    </StackLayout>
    <StackLayout row="1" className="menuItem main-color" orientation="horizontal" onTap="tapSMN" style="margin-bottom:.5%;" id="abc">
      <RL:Ripple rippleColor="#ff8888" class="drawerRipple"   tap="tapSMN">
        <StackLayout orientation="horizontal">
          <StackLayout class="menuItemLabel">
            <Label text="SCHEDULE ME NOW"/>
          </StackLayout>
          <StackLayout class="menuItemIcon">
            <Label text="&#xf105;" class="icon" />
          </StackLayout>
        </StackLayout>
      </RL:Ripple>
    </StackLayout>
    <StackLayout row="2" className="menuItem main-color" orientation="horizontal" tap="tapFindADoctor" style="margin-bottom:.5%;">
      <RL:Ripple rippleColor="#ff8888" class="drawerRipple"   tap="tapFindADoctor">
        <StackLayout orientation="horizontal">
          <StackLayout class="menuItemLabel">
            <Label text="FIND A DOCTOR"/>
          </StackLayout>
          <StackLayout class="menuItemIcon">
            <Label text="&#xf105;" class="icon" />
          </StackLayout>
        </StackLayout>
      </RL:Ripple>
    </StackLayout>
    <StackLayout row="3" className="menuItem main-color" orientation="horizontal" tap="tapFindALocation" style="margin-bottom:.5%;">
      <RL:Ripple rippleColor="#ff8888" class="drawerRipple"   tap="tapFindALocation">
        <StackLayout orientation="horizontal">
          <StackLayout class="menuItemLabel">
            <Label text="FIND A LOCATION" />
          </StackLayout>
          <StackLayout class="menuItemIcon">
            <Label text="&#xf105;" class="icon" />
          </StackLayout>
        </StackLayout>
      </RL:Ripple>
    </StackLayout>
    <StackLayout row="4" className="menuItem main-color" orientation="horizontal" tap="tapFindERUrgentCare" style="margin-bottom:.5%;">
      <RL:Ripple rippleColor="#ff8888" class="drawerRipple"   tap="tapFindERUrgentCare">
        <StackLayout orientation="horizontal">
          <StackLayout class="menuItemLabel">
            <Label text="FIND ER/URGENT CARE" />
          </StackLayout>
          <StackLayout class="menuItemIcon">
            <Label text="&#xf105;" class="icon" />
          </StackLayout>
        </StackLayout>
      </RL:Ripple>
    </StackLayout>
    <StackLayout row="5" className="menuItem main-color" orientation="horizontal" tap="tapSymptomChecker" style="margin-bottom:.5%;">
      <RL:Ripple rippleColor="#ff8888" class="drawerRipple"   tap="tapSymptomChecker">
        <StackLayout orientation="horizontal">
          <StackLayout class="menuItemLabel">
            <Label text="SYMPTOM CHECKER"  />
          </StackLayout>
          <StackLayout class="menuItemIcon">
            <Label text="&#xf105;" class="icon" />
          </StackLayout>
        </StackLayout>
      </RL:Ripple>
    </StackLayout>
    <StackLayout row="6" className="menuItem main-color" orientation="horizontal" tap="tapContactUH" style="margin-bottom:.5%;">
      <RL:Ripple rippleColor="#ff8888" class="drawerRipple"   tap="tapContactUH">
        <StackLayout orientation="horizontal">
          <StackLayout class="menuItemLabel">
            <Label text="CONTACT UH" />
          </StackLayout>
          <StackLayout class="menuItemIcon">
            <Label text="&#xf105;" class="icon" />
          </StackLayout>
        </StackLayout>
      </RL:Ripple>
    </StackLayout>
    <StackLayout row="7" className="menuItem main-color" orientation="horizontal"  tap="tapTermsOfUse"  style="margin-bottom:.5%;">
      <RL:Ripple rippleColor="#ff8888" class="drawerRipple"   tap="tapTermsOfUse">
