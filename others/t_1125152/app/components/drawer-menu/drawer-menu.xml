            <Label text="ABOUT UH NOW" />
          </StackLayout>
          <StackLayout class="menuItemIcon">
            <Label text="&#xf105;" class="icon" />
          </StackLayout>
        </StackLayout>
      </RL:Ripple>
    </StackLayout>

